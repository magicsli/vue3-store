{
  "compilerOptions": {
    "paths": {
      "/@/*": ["src/*"]
    },
    "baseUrl": ".",
    "moduleResolution":"Node",
    "lib": ["esnext", "dom", "dom.iterable", "scripthost", "ES2020"],
    "module": "esnext"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/types/images.d.ts",
    "tests/**/*.ts",
    "tests/**/*.tsx", 
    "src/route/index.js"  ],
